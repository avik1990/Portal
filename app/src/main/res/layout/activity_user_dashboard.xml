            android:layout_marginEnd="5dp"
            app:cardCornerRadius="8dp"
            android:layout_marginStart="5dp"
            app:cardUseCompatPadding="true"
            app:cardBackgroundColor="#703770"
            app:layout_constraintEnd_toStartOf="@+id/guideline1"
