                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="5dp"
                    android:singleLine="true"
                    android:text="@string/register_profile_edit_login"
                    android:paddingEnd="3dp"
                    android:visibility="gone"
                    android:textColor="@color/black"
                    android:textSize="12sp"
                    >
                </TextView>
            </LinearLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardview_feedback"
            app:cardBackgroundColor="#1F3F49"
            android:layout_width="0dp"
            android:layout_height="130dp"
            app:layout_constraintTop_toBottomOf="@+id/pmthist"
            app:layout_constraintStart_toEndOf="@+id/guideline1"
            android:layout_marginEnd="5dp"
            app:cardCornerRadius="8dp"
            android:layout_marginStart="5dp"
            app:cardUseCompatPadding="true"
            app:layout_constraintEnd_toStartOf="@+id/guidelin">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center_horizontal"
                android:gravity="center_horizontal"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:gravity="center_horizontal">

                    <ImageView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginTop="20dp"
                        android:src="@drawable/feedback" />
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:gravity="center_horizontal"
                    android:paddingStart="5dp"
                    android:text="@string/feedback"
                    android:textColor="@color/White"
                    android:textSize="12sp"></TextView>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingStart="5dp"
                    android:paddingEnd="3dp"
                    android:singleLine="true"
                    android:text="@string/register_profile_edit_login"
                    android:textColor="@color/black"
                    android:textSize="12sp"
                    android:visibility="gone"></TextView>
            </LinearLayout>
        </androidx.cardview.widget.CardView>





